                                   id="{{ idPrefix ~ prefix }}AddressPhoneNumber"
                                   placeholder="{{ 'address.phoneNumberPlaceholder'|trans|striptags }}"
                                   {# @deprecated tag:v6.7.0 - Input name prefix changes #}
                                   {% if feature('ADDRESS_SELECTION_REWORK') %}
                                       name="{% if prefix %}{{ prefix }}[phoneNumber]{% else %}phoneNumber{% endif %}"
                                   {% else %}
                                       name="{{ prefix }}[phoneNumber]"
                                   {% endif %}
                                   value="{{ data.get('phoneNumber') }}"
